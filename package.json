{
	"name": "wow-api",
	"displayName": "WoW API",
	"description": "World of Warcraft AddOn support",
	"version": "0.3.0",
	"publisher": "Ketho",
	"repository": {
		"type": "git",
		"url": "https://github.com/Ketho/vscode-wow-api.git"
	},
	"license": "MIT",
	"engines": {
		"vscode": "^1.53.0"
	},
	"categories": [
		"Linters",
		"Programming Languages"
	],
	"keywords": [
		"Lua",
		"WoW",
		"World of Warcraft"
	],
	"icon": "img/wow-logo.png",
	"activationEvents": [
		"*"
	],
	"main": "./out/extension.js",
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js",
		"vscode:uninstall": "node ./out/src/uninstall"
	},
	"devDependencies": {
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.2.1",
		"@types/node": "^14.14.31",
		"@types/vscode": "^1.53.0",
		"@typescript-eslint/eslint-plugin": "^4.15.2",
		"@typescript-eslint/parser": "^4.15.2",
		"eslint": "^7.21.0",
		"glob": "^7.1.6",
		"mocha": "^8.3.0",
		"typescript": "^4.2.2",
		"vscode-test": "^1.5.1"
	},
	"extensionDependencies": [
		"sumneko.lua"
	]
}
