{
	"name": "wow-api",
	"displayName": "WoW API",
	"description": "Adds support for World of Warcraft AddOn API",
	"version": "0.1.0",
	"publisher": "Ketho",
	"repository": "https://github.com/Ketho/vscode-wow-api",
	"license": "MIT",
	"engines": {
		"vscode": "^1.44.0"
	},
	"categories": [
		"Linters",
		"Programming Languages"
	],
	"keywords": [
		"Lua",
		"WoW",
		"World of Warcraft"
	],
	"icon": "Images/wow-logo.png",
	"activationEvents": [
		"*"
	],
	"main": "./out/extension.js",
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js",
		"vscode:uninstall": "node ./out/src/uninstall"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"@types/vscode": "^1.44.0",
		"@typescript-eslint/eslint-plugin": "^2.26.0",
		"@typescript-eslint/parser": "^2.26.0",
		"eslint": "^6.8.0",
		"glob": "^7.1.6",
		"mocha": "^7.1.1",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0"
	},
	"extensionDependencies": [
		"sumneko.lua"
	]
}
